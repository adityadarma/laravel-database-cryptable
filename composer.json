{
    "name": "adityadarma/laravel-database-cryptable",
    "description": "Cryptable value database for laravel",
    "keywords": ["laravel", "database cryptable", "laravel cryptable", "encrypt decrypt"],
    "homepage": "https://github.com/adityadarma/laravel-database-cryptable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Darma",
            "email": "adhit.boys1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AdityaDarma\\LaravelDatabaseCryptable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdityaDarma\\LaravelDatabaseCryptable\\LaravelDatabaseCryptableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
